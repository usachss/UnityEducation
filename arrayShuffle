using System;

namespace normShuffle
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] arrayToShuffle = {1,3,4,6,7};
            Shuffle(ref arrayToShuffle);
            
            for (int i = 0; i < arrayToShuffle.Length; i++)
            {
                Console.Write("\n" + arrayToShuffle[i]);
            }
        }
        
        static void Shuffle(ref int[] arrayToShuffle)
        {
            Random rand = new Random();
            for (int i = arrayToShuffle.Length-1; i > 0; i--)
            {
                int j = rand.Next(i+1);
                int k = arrayToShuffle[j];
                arrayToShuffle[j] = arrayToShuffle[i];
                arrayToShuffle[i] = k;
               // Console.Write("\n"+arrayToShuffle[i]);
            }
        }
    }
}
